{
  "openapi": "3.0.1",
  "info": {
    "title": "OpenAPI Plant Store",
    "description": "A sample API that uses a plant store as an example to demonstrate features in the OpenAPI specification",
    "license": {
      "name": "MIT"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://jump-backend-playground.onrender.com/api"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "paths": {
    "/skyclub/applications": {
      "get": {
        "summary": "Retrieve all SkyClub applications",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Application"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/emails/subscribe": {
      "post": {
        "summary": "Add a subscriber to a Campaign Monitor list",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "description": "The email of the subscriber"
                  },
                  "listId": {
                    "type": "string",
                    "description": "The ID of the list to which the subscriber should be added"
                  },
                  "name": {
                    "type": "string",
                    "description": "Name of the subscriber"
                  },
                  "customFields": {
                    "type": "object",
                    "description": "Additional custom fields for the subscriber",
                    "additionalProperties": true
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Subscriber added successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/emails/send": {
      "post": {
        "summary": "Send a transactional email using Campaign Monitor",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "smartEmailId": {
                    "type": "string",
                    "description": "The ID of the smart email template in Campaign Monitor"
                  },
                  "to": {
                    "type": "string",
                    "description": "Recipient's email address"
                  },
                  "data": {
                    "type": "object",
                    "description": "Additional data or parameters for the email",
                    "additionalProperties": true
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Email sent successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/EmailResponse"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Plant": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "description": "The name of the plant",
            "type": "string"
          },
          "tag": {
            "description": "Tag to specify the type",
            "type": "string"
          }
        }
      },
      "NewPlant": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Plant"
          },
          {
            "required": [
              "id"
            ],
            "type": "object",
            "properties": {
              "id": {
                "description": "Identification number of the plant",
                "type": "integer",
                "format": "int64"
              }
            }
          }
        ]
      },
      "Error": {
        "required": [
          "error",
          "message"
        ],
        "type": "object",
        "properties": {
          "error": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "Application": {
        "type": "object",
        "properties": {
          "lastName": {
            "type": "string"
          },
          "linkedIn": {
            "type": "string"
          },
          "discord": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "votes": {
            "type": "integer"
          },
          "city": {
            "type": "string"
          },
          "companyPosition": {
            "type": "string"
          },
          "company": {
            "type": "string"
          },
          "aboutDesc": {
            "type": "string"
          },
          "acceptedBy": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "UUPuuid": {
            "type": "string"
          },
          "timeAdded": {
            "type": "string"
          },
          "wallet": {
            "type": "string"
          },
          "pk": {
            "type": "string"
          }
        }
      },
      "Response": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "EmailResponse": {
        "type": "object",
        "properties": {
          "Status": {
            "type": "string"
          },
          "MessageID": {
            "type": "string"
          },
          "Recipient": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}